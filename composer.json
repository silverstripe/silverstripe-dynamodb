{
	"name": "silverstripe/dynamodb",
	"description": "SilverStripe DynamoDB integration.",
	"type": "silverstripe-module",
	"keywords": ["silverstripe", "aws", "dynamodb"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "SilverStripe",
			"homepage": "http://silverstripe.com"
		},
		{
			"name": "The SilverStripe Community",
			"homepage": "http://silverstripe.org"
		}
	],
	"require": {
		"ext-apcu": "*",
		"aws/aws-sdk-php": "~3.18",
		"doctrine/cache": "1.*",
		"silverstripe/crontask": "*",
		"silverstripe/framework": "^4@dev"
	},
	"require-dev": {
		"phpunit/PHPUnit": "^5.7"
	},
	"autoload": {
    "psr-4": {
      "SilverStripe\\DynamoDb\\": "code/",
      "SilverStripe\\DynamoDb\\Tests\\": "tests/"
    }
  },
	"extra": {
		"installer-name": "silverstripe-dynamodb",
		"branch-alias": {
			"dev-master": "3.0.x-dev"
		}
	},
	"scripts": {
		"lint": "phpcs code/ tests/"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
